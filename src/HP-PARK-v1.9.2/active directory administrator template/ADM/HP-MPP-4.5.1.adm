          DEFCHECKED
          VALUENAME "DriverUpdates"     
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Enable online support"           CHECKBOX
          DEFCHECKED
          VALUENAME "OnlineSupport"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Enable supply ordering"          CHECKBOX
          DEFCHECKED
          VALUENAME "OrderSupplies"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Enable product manuals"          CHECKBOX
          DEFCHECKED
          VALUENAME "ProductManuals"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART
