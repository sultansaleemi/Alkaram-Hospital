CLASS USER

CATEGORY "HP Managed Print Policies 4.5.1"

; UPD Administrative Template version xxxx.xx 
; 
; Most applications will use a registry key with the naming convention 
; Software\Policies\CompanyName\ProductName\ComponentName
; 
; For example:  Software\Policies\Hewlett-Packard\HP Print Settings
;               Software\Microsoft\Windows\CurrentVersion\Policies\blah\blah
;
; 061103 - Commented out EXPLAIN due to incompatability with Server 2000
; 
; 
; 

   KEYNAME "Software\Policies\Hewlett-Packard\HP Print Settings\ADSPrintPolicy"

CATEGORY "UseMode Settings"
 
; General Settings
  
   POLICY "General Settings"
   EXPLAIN !!t_General_Settings

         VALUENAME "General_Settings"	
      PART !!p_DisableColor         CHECKBOX
         VALUENAME "Color"
           VALUEON NUMERIC 0
           VALUEOFF NUMERIC 1
      END PART

      PART "Show device address"          CHECKBOX
         DEFCHECKED
         VALUENAME "ShowDeviceAddress"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART
	  PART "" TEXT END PART

      PART "Enable Policy for all users"          CHECKBOX
         DEFCHECKED
	 KEYNAME "Software\Hewlett-Packard\HP Print Settings"
         VALUENAME "UPDADS"
           VALUEON  NUMERIC 19728374
           VALUEOFF NUMERIC 0
      END PART
      PART "ver. 100811.00" TEXT END PART
   END POLICY

; Restricted mode

   POLICY "User Interaction Settings"
      EXPLAIN !!t_Restricted_Mode
;      VALUENAME "Restricted"
;           VALUEON  NUMERIC 0
;           VALUEOFF NUMERIC 1

      VALUENAME "UserInteractionSettings"
      PART "Restricted Mode"      CHECKBOX
;         DEFCHECKED
         VALUENAME "Restricted"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Show UPD dialog settings window"      CHECKBOX
         DEFCHECKED
         VALUENAME "ShowDriverSettings"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Allow user to group printers by location"         CHECKBOX
         DEFCHECKED
         VALUENAME "ShowProfiles"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Allow user to add printers to OS Printers folder" CHECKBOX
         DEFCHECKED
         VALUENAME "ShowMakePermanent"
           VALUEON  NUMERIC 1
           VALUEOFF NUMERIC 0
      END PART

      PART "Device verify level:" DROPDOWNLIST REQUIRED
        VALUENAME "DeviceVerifyLevel"
	ITEMLIST
	   NAME "(Do not specify)" DEFAULT
	   VALUE "default"
	   NAME "Verify communication and compatibility with driver (High)"
	   VALUE "high"
	   NAME "Verify device ID and Model Name can be obtained (Medium)"
	   VALUE "medium"
	   NAME "Verify port can be opened for printing (Low)"
	   VALUE "low"
	END ITEMLIST 
      END PART

      PART "When printing:" DROPDOWNLIST REQUIRED
        VALUENAME "WhenPrinting"
	ITEMLIST
	   NAME "(Do not specify)" DEFAULT
	   VALUE "default"
	   NAME "Always prompt for the destination when printing a job"
	   VALUE "always"
	   NAME "Only prompt the first time a job is printed from an application"
	   VALUE "firstjob"
	   NAME "Only prompt if the last known destination is no longer available"
	   VALUE "onerror"
	END ITEMLIST 
      END PART

   END POLICY

;Enable services tab

   POLICY "Enable services tab"    
